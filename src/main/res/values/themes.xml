<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Artrade" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/artrade_gold</item>
        <item name="colorPrimaryDark">@color/artrade_gray</item>
        <item name="colorAccent">@color/artrade_gold</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Artrade.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Artrade.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>